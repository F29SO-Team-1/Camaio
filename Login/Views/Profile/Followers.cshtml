@model Login.Models.Followers.FollowersModelList

@{
    ViewData["Title"] = "Followers Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container">
    @foreach (var user in Model.UserList)
    {
        @user.Username.UserName <br>
        <img src="@user.Username.ProfileImageUrl" style="width: 20%; height:20%;" alt="ProfilePic" />
    }
</div>
